set term svg background rgb "white"
set out "Gamma.svg"
set xlabel "x"
set ylabel "y"
set key bottom right
set tics out
set xzeroaxis
set yzeroaxis
set title "Gamma function"
plot [-5:5][-5:5]\
        "out.gamma.txt" with lines title "Gamma function" \
        , "out.gamma.test.txt" with points pt 7 title "True gamma"